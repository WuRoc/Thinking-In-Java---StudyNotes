//: P04_DataOnly.java

package com.wuroc.chaptertwo;

/**
 * @author HepengWu
 * @author www.github.com/WuRoc
 * @version 1.0
 * @2020年6月3日
 *
 * 
 */
public class P04_DataOnly {
	int i;
	float f;
	double d;
	boolean b;
	long l;
	
	public static void main(String[] arg) {

		P04_DataOnly p = new P04_DataOnly();
//		objectReference.member
		p.i = 47;
//		float 需要加f  或者  向上转型  p.f = (float) 0.0;
//		在java里面，没小数点的默认是int,有小数点的默认是 double
		
		p.f = 0.0f;
		p.d = 1;
		p.b =false;
		p.l = (long) 0.2;
		
		System.out.println("p.i="+p.i);
		System.out.println("p.f="+p.f);
		System.out.println("p.d="+p.d);
		System.out.println("p.b="+p.b);
	}

}///:~
