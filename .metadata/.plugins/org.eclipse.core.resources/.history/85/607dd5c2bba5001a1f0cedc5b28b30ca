public class P10ShowArgs {
	
	public static void main(String[] args) {
		
		String[] name= {"A","B","C"};
		
		if(args.length <3) {
			
			System.err.println("Need 3 arguments ");
			
			System.exit(1);
		}
		
		System.out.println(args[0]);
		
		System.out.println(args[1]);
		
		System.out.println(args[2]);
		
	} 
	
	
	
	

}
