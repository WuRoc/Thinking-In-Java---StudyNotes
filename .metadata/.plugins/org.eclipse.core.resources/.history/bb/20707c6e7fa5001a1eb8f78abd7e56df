 */
public class P07_Increment {
	
	static int  i = 1010;
	
	static void increment() {
		P07_Increment.i++;
		
	}
	public static void main(String[] args) {
		P07_Increment p = new P07_Increment();
		p.increment();
	}
	
	
	
	
	
	
	
	
}
